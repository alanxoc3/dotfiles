[simple]
command=hostname
interval=-1
color=#FFFFFF

[battery]
color=#FFFFFF
command=~/.dotfiles/bin/i3b_battery
interval=30
label=电量 

[memory]
color=#77AAFF
command=~/.dotfiles/bin/i3b_memory
label=内存 
interval=30

[wifi]
color=#FFFFFF
command=~/.dotfiles/bin/i3b_wifi
interval=30
label=无线 
