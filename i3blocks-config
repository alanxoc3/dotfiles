[simple]
command=hostname
interval=-1
color=#FFFFFF

[battery]
color=#FFFFFF
command=~/.dotfiles/bin/i3b_battery
interval=30
label=bat: 

[memory]
color=#77AAFF
command=~/.dotfiles/bin/i3b_memory
label=mem: 
interval=30

[wifi]
color=#FFFFFF
command=~/.dotfiles/bin/i3b_wifi
interval=30
label=wifi: 
INTERFACE=wlo1

[time]
color=#FFFFFF
command=~/.dotfiles/bin/i3b_time
interval=1
STRFTIME_FORMAT=%Y-%m-%d %H:%M:%S
